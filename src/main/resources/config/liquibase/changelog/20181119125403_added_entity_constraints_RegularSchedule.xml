<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog ../xml-schema/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity RegularSchedule.
    -->
    <changeSet id="20181119125403-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="srvc_id"
                                 baseTableName="regular_schedule"
                                 constraintName="fk_regular_schedule_srvc_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service"/>

        <addForeignKeyConstraint baseColumnNames="location_id"
                                 baseTableName="regular_schedule"
                                 constraintName="fk_regular_schedule_location_id"
                                 referencedColumnNames="id"
                                 referencedTableName="location"/>

        <addForeignKeyConstraint baseColumnNames="service_atlocation_id"
                                 baseTableName="regular_schedule"
                                 constraintName="fk_regular_schedule_service_atlocation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="service_at_location"/>

    </changeSet>
</databaseChangeLog>
